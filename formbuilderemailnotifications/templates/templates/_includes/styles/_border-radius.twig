<div class="option-item styles-item">
    <span class="option-title">
        <span class="option-text">{{ "Border Radius" |t }}</span>
    </span>
    <div class="option-actions">
        <input id="templateBorderRadius" type="range" name="styles[border][radius]" min="5" max="100" step="5" value="{{ template.styles.border.radius is defined ? template.styles.border.radius : '5' }}" />
    </div>
</div>